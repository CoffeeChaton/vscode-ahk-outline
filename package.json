{
    "name": "vscode-autohotkey-outline",
    "displayName": "AutoHotkey Outline",
    "description": "AutoHotkey Outline of VSCode",
    "version": "2.3.1",
    "publisher": "CoffeeChaton",
    "engines": {
        "vscode": "^1.45.0"
    },
    "activationEvents": [
        "onLanguage:ahk"
    ],
    "main": "./out/extension",
    "homepage": "https://github.com/CoffeeChaton/vscode-ahk-outline/blob/master/README.md",
    "bugs": {
        "url": "https://github.com/CoffeeChaton/vscode-ahk-outline/issues"
    },
    "categories": [
        "Programming Languages",
        "Snippets"
    ],
    "license": "SEE LICENSE IN LICENSE.md",
    "icon": "icon.png",
    "galleryBanner": {
        "color": "#5c2d91",
        "theme": "dark"
    },
    "keywords": [
        "vscode",
        "autohotkey",
        "ahk"
    ],
    "contributes": {
        "breakpoints": [
            {
                "language": "ahk"
            }
        ],
        "commands": [
            {
                "command": "ahk.bar.click",
                "title": "TODO AhkOutline StatusBar Click"
            }
        ],
        "configuration": {
            "type": "object",
            "title": "AhkOutline",
            "properties": {
                "AhkOutline.statusBar.showVersion": {
                    "type": "boolean",
                    "default": false,
                    "description": "show ahk outline version at status bar, default is false."
                },
                "AhkOutline.statusBar.showTime": {
                    "type": "boolean",
                    "default": true,
                    "description": "display resolution time as millisecond at status bar, default is true."
                },
                "AhkOutline.statusBar.showFileName": {
                    "type": "boolean",
                    "default": false,
                    "description": "display your ahk file name at status bar, default is false."
                },
                "AhkOutline.statusBar.displayColor": {
                    "type": "string",
                    "default": "#e783e1",
                    "description": "color of status bar, use '' to use theme color,or use HTML Color, exp: '#D382FF' or 'rgb(255,255,100)"
                },
                "AhkOutline.isAHKv2": {
                    "type": "boolean",
                    "default": false,
                    "description": "if your ahk version is not v2,please don't Tick this."
                },
                "AhkOutline.hover.showParm": {
                    "type": "boolean",
                    "default": true,
                    "description": "when hover a function to show this Parm."
                },
                "AhkOutline.hover.showComment": {
                    "type": "boolean",
                    "default": true,
                    "description": "when hover a function to show this Comment."
                }
            }
        },
        "languages": [
            {
                "id": "ahk",
                "aliases": [
                    "AutoHotkey",
                    "autohotkey",
                    "ahk"
                ],
                "extensions": [
                    ".ahk",
                    ".ext"
                ],
                "configuration": "./ahk.configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "ahk",
                "scopeName": "source.ahk",
                "path": "./syntaxes/ahk.tmLanguage.json"
            }
        ],
        "snippets": [
            {
                "language": "ahk",
                "path": "./snippets/ahk.json"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "@babel/plugin-proposal-do-expressions": "^7.10.4",
        "@types/glob": "^7.1.3",
        "@types/mocha": "^7.0.1",
        "@types/node": "^14.0.27",
        "@types/vscode": "^1.47.0",
        "@typescript-eslint/eslint-plugin": "^3.7.1",
        "@typescript-eslint/parser": "^3.7.1",
        "eslint": "^7.5.0",
        "eslint-config-airbnb": "^18.2.0",
        "eslint-config-airbnb-base": "^14.2.0",
        "eslint-config-airbnb-typescript": "^8.0.2",
        "eslint-config-prettier": "^6.11.0",
        "eslint-plugin-chai-expect": "^2.2.0",
        "eslint-plugin-import": "^2.22.0",
        "eslint-plugin-jsx-a11y": "^6.3.1",
        "eslint-plugin-no-setter-return": "^0.1.2",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-prettier": "^3.1.4",
        "eslint-plugin-promise": "^4.2.1",
        "eslint-plugin-react": "^7.20.5",
        "eslint-plugin-security": "^1.4.0",
        "eslint-plugin-typescript": "^0.14.0",
        "generator-code": "^1.2.19",
        "get-port": "^5.1.1",
        "glob": "^7.1.6",
        "mocha": "^8.1.0",
        "prettier": "^2.0.5",
        "prettier-eslint": "^11.0.0",
        "prettier-eslint-cli": "^5.0.0",
        "typescript": "^3.9.7",
        "typescript-eslint-parser": "^22.0.0",
        "vscode-debugadapter": "^1.41.1",
        "vscode-test": "^1.4.0",
        "xml2js": "^0.4.23"
    },
    "dependencies": {
        "fs": "0.0.2",
        "vscode-oniguruma": "^1.3.1"
    }
}
