{
    "name": "vscode-autohotkey-neko-help",
    "displayName": "AutoHotkeyNekoHelp",
    "description": "AutoHotkey help of VSCode",
    "version": "2.3.1",
    "publisher": "CoffeeChaton",
    "engines": {
        "vscode": "^1.45.0"
    },
    "activationEvents": [
        "onLanguage:ahk"
    ],
    "main": "./out/extension",
    "homepage": "https://github.com/CoffeeChaton/vscode-ahk-outline/blob/master/README.md",
    "bugs": {
        "url": "https://github.com/CoffeeChaton/vscode-ahk-outline/issues"
    },
    "categories": [
        "Programming Languages",
        "Snippets"
    ],
    "license": "SEE LICENSE IN LICENSE.md",
    "icon": "icon.png",
    "galleryBanner": {
        "color": "#5c2d91",
        "theme": "dark"
    },
    "keywords": [
        "vscode",
        "autohotkey",
        "ahk"
    ],
    "contributes": {
        "breakpoints": [
            {
                "language": "ahk"
            }
        ],
        "commands": [
            {
                "command": "ahk.bar.click",
                "title": "AhkNekoHelp StatusBar Click clear cash"
            }
        ],
        "configuration": {
            "type": "object",
            "title": "AhkNekoHelp",
            "properties": {
                "AhkNekoHelp.statusBar.showVersion": {
                    "type": "boolean",
                    "default": false,
                    "description": "show ahk NekoHelp version at status bar, default is false."
                },
                "AhkNekoHelp.statusBar.showTime": {
                    "type": "boolean",
                    "default": true,
                    "description": "display resolution time as millisecond at status bar, default is true."
                },
                "AhkNekoHelp.statusBar.showFileName": {
                    "type": "boolean",
                    "default": false,
                    "description": "display your ahk file name at status bar, default is false."
                },
                "AhkNekoHelp.statusBar.displayColor": {
                    "type": "string",
                    "default": "",
                    "description": "color of status bar, use '' to use theme color,or use HTML Color, exp: '#e783e1' or 'rgb(255,255,100)"
                },
                "AhkNekoHelp.hover.showComment": {
                    "type": "boolean",
                    "default": true,
                    "description": "when hover a function to show this Comment."
                },
                "AhkNekoHelp.format.textReplace": {
                    "type": "boolean",
                    "default": true,
                    "description": "auto call textReplace & diff"
                },
                "AhkNekoHelp.lint.funcSize": {
                    "type": "number",
                    "default": 100,
                    "description": "lint of function/Method size (line)"
                }
            }
        },
        "languages": [
            {
                "id": "ahk",
                "aliases": [
                    "AutoHotkey",
                    "autohotkey",
                    "ahk"
                ],
                "extensions": [
                    ".ahk",
                    ".ext"
                ],
                "configuration": "./ahk.configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "ahk",
                "scopeName": "source.ahk",
                "path": "./syntaxes/ahk.tmLanguage.json"
            }
        ],
        "snippets": [
            {
                "language": "ahk",
                "path": "./snippets/ahk.json"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "@types/benchmark": "^1.0.33",
        "@types/glob": "^7.1.3",
        "@types/mocha": "^7.0.1",
        "@types/node": "^14.14.12",
        "@types/temp": "^0.8.34",
        "@types/vscode": "^1.51.0",
        "@typescript-eslint/eslint-plugin": "^3.10.1",
        "@typescript-eslint/parser": "^3.10.1",
        "benchmark": "^2.1.4",
        "eslint": "^7.15.0",
        "eslint-config-airbnb": "^18.2.1",
        "eslint-config-airbnb-base": "^14.2.1",
        "eslint-config-airbnb-typescript": "^8.0.2",
        "eslint-config-prettier": "^6.15.0",
        "eslint-plugin-chai-expect": "^2.2.0",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-jsx-a11y": "^6.4.1",
        "eslint-plugin-no-setter-return": "^0.1.2",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-prettier": "^3.2.0",
        "eslint-plugin-promise": "^4.2.1",
        "eslint-plugin-react": "^7.21.5",
        "eslint-plugin-security": "^1.4.0",
        "eslint-plugin-typescript": "^0.14.0",
        "generator-code": "^1.3.9",
        "mocha": "^8.2.1",
        "prettier": "^2.2.1",
        "prettier-eslint": "^11.0.0",
        "prettier-eslint-cli": "^5.0.0",
        "typescript": "^3.9.7",
        "typescript-eslint-parser": "^22.0.0",
        "vscode-debugadapter": "^1.43.0",
        "vscode-languageserver-protocol": "^3.15.3",
        "vscode-languageserver-types": "^3.15.1",
        "vscode-oniguruma": "^1.4.0",
        "vscode-test": "^1.4.1"
    },
    "dependencies": {
        "fs": "0.0.2",
        "madge": "^3.12.0",
        "temp": "^0.9.4",
        "vscode-languageserver-textdocument": "^1.0.1",
        "vscode-languageserver": "^6.1.1"
    }
}
